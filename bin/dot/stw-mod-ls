#!/usr/bin/env sh
# List stow modules for the current system

mods='base git wm'

case "$(uname -n)" in
    X13)
        mods="$mods arch"
        ;;
    amd)
        mods="$mods"
        ;;
    l4)
        mods="base"
        ;;
    IT*)
        mods="base git mac"
        ;;
    *)
        mods="$mods"
        ;;
esac

echo "$mods"
